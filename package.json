  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/HaojiXu/BetterWeb/issues"
  },
  "homepage": "https://github.com/HaojiXu/BetterWeb#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "2.1.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "sass": "^1.17.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
